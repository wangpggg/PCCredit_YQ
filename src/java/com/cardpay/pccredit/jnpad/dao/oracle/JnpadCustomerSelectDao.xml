<!-- 根据cardId -->
<select id="selectCustomerInforByCardId" resultMap="selectCustomerinfo">


		SELECT * FROM
		(SELECT
		ROW_.*,ROWNUM ROWNUM_
		FROM (
		select *
		from BASIC_CUSTOMER_INFORMATION c
		where 1=1
		AND
		( c.CARD_ID=#{cardId})
		) ROW_)

</select>
<!-- 根据id -->
<select id="selectCustomerInfoById" resultMap="selectCustomerJnInfo">
		SELECT * FROM
		(SELECT
		ROW_.*,ROWNUM ROWNUM_
		FROM (
		select *
		from T_CIPERSONBASINFO c
		where 1=1
		AND
		( c.ID=#{id})
		) ROW_)

</select>
<!-- 根据内码 -->
<select id="selectCustomerByNm" resultMap="selectCustomerJnInfo">
		SELECT * FROM
		(SELECT
		ROW_.*,ROWNUM ROWNUM_
		FROM (
		select *
		from T_CIPERSONBASINFO c
		where 1=1
		AND
		( c.CUSTID=#{custid})
		) ROW_)

